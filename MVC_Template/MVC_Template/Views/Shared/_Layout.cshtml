                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>

                    </div>
                </div>
            </div>
        </div>
        <div id="body-container">
            <div id="right-column">
                <div class="mainnav-horizontal-outer">
                    <div id="mainnav-horizontal-inner" class="fixed">
                        <!-- NOTE TO READER: Accepts the following class(es) "menu-text-only", "fixed" class -->
                        <div id="mainnav-horizontal">
                            <ul class="mainnav-horizontal">
                                <!-- NOTE TO READER: Accepts the following class(es) "animate" class -->
                                <li class="menu-item-top">
                                    <a href="/Home/Index" class="top">
                                        <span class="main-menu-icon">
                                            <span aria-hidden="true" class="icon icon-grid-big icon-size-medium"></span>
                                        </span>
                                        <span class="main-menu-text">Ana Sayfa</span>
                                    </a>
                                </li>
                                <li class="menu-item-top">
                                    <a href="/Product/Index" class="top">
                                        <span class="main-menu-icon">
                                            <span aria-hidden="true" class="icon icon-dollar icon-size-medium"></span>
                                        </span>
                                        <span class="main-menu-text">Ürünler</span>
                                    </a>
                                    <ul>
                                        <li><a href="/Product/Index">Ürün Listele</a></li>
                                        <li><a href="/Product/AddProduct">Ürün Ekle</a></li>
                                    </ul>
                                </li>
                                <li class="menu-item-top">
                                    <a href="/Category/Index" class="top">
                                        <span class="main-menu-icon">
                                            <span aria-hidden="true" class="icon icon-layers icon-size-medium"></span>
                                        </span>
                                        <span class="main-menu-text">Categories</span>
                                    </a>

                                </li>
                                <li class="menu-item-top">
                                    <a href="/Supplier/Index" class="top">
                                        <span class="main-menu-icon">
                                            <span aria-hidden="true" class="icon icon-layers icon-size-medium"></span>
                                        </span>
                                        <span class="main-menu-text">Tedarikçiler</span>
                                    </a>

                                </li>
                                <li class="menu-item-top">
                                    <a href="/Order/Index" class="top">
                                        <span class="main-menu-icon">
                                            <span aria-hidden="true" class="icon icon-layers icon-size-medium"></span>
                                        </span>
                                        <span class="main-menu-text">Siparişler</span>
                                    </a>

                                </li>
                                <li class="menu-item-top">
                                    <a href="/OrderDetail/Index" class="top">
                                        <span class="main-menu-icon">
                                            <span aria-hidden="true" class="icon icon-layers icon-size-medium"></span>
                                        </span>
                                        <span class="main-menu-text">Sipariş Detayları</span>
                                    </a>

                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="right-column-content" style="padding-top: 58px;">
                    @RenderBody()
                </div>
                <div id="footer-container">
                    <div class="footer-content">
                        &copy; <a href="#?ref=Base5Builder">Circloid</a> was made with <span style="color:#FF0000;">&#10084;</span> and supported by - <a href="http://base5builder.com/" target="_blank" style="font-weight:300;color:#ffffff;background:#1d1d1d;padding:0 3px;">Base<span style="color:#ffa733;font-weight:bold">5</span>Builder</a>
                    </div>
                </div>
            </div>
        </div>
    </div><!-- /.container -->
    <!-- Default Modal Add -->
    <div class="modal fade" id="default-modal" tabindex="-1" role="dialog" aria-labelledby="default-modal-label" aria-hidden="true" style="display: none;">
        <div class="modal-dialog">
            <div class="modal-content" data-border-top="multi" style="border-top: 0px;">
                <div class="top-border"><span class="border-block bg-color-green"></span><span class="border-block bg-color-orange"></span><span class="border-block bg-color-yellow"></span><span class="border-block bg-color-blue"></span><span class="border-block bg-color-red"></span><span class="border-block bg-color-lime"></span><span class="border-block bg-color-purple"></span></div><div class="top-border"><span class="border-block bg-color-green"></span><span class="border-block bg-color-orange"></span><span class="border-block bg-color-yellow"></span><span class="border-block bg-color-blue"></span><span class="border-block bg-color-red"></span><span class="border-block bg-color-lime"></span><span class="border-block bg-color-purple"></span></div>
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                    <h4 class="modal-title" id="default-modal-label">Model Silme</h4>
                </div>
                <div class="modal-body">
                    <p>Silmek İstedğinize Emin Misiniz ?</p>
                    <p class="Message"></p>
                    <p class="Error"></p>

                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Kapat</button>
                    <button type="button" class="btn btn-danger" tedModalDelete>Sil</button>
                </div>
            </div><!-- /.modal-content -->
        </div><!-- /.modal-dialog -->
    </div>
    <!-- END Modal Add -->
